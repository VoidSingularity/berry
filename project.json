{
    "packages": {
        "loader": {
            "source": "loader"
        },
        "agent": {
            "dep": [ "loader" ],
            "source": "agent",
            "manifest": "berryagent"
        }
    },
    "runs": {
        "download_manifest": {
            "function": "download_manifest",
            "repeat_everytime": false
        },
        "build_all": {
            "dep": [
                "loader",
                "agent"
            ],
            "repeat_everytime": true
        },
        "download_minecraft": {
            "dep": [
                "download_manifest",
                "build_all"
            ],
            "function": "download_minecraft",
            "repeat_everytime": false
        },
        "deobfuscate": {
            "dep": [
                "download_minecraft"
            ],
            "function": "deobfuscate",
            "repeat_everytime": false
        },
        "download_dependencies": {
            "dep": [
                "download_minecraft"
            ],
            "function": "download_dependencies",
            "repeat_everytime": false
        },
        "setup_vscode": {
            "dep": [
                "download_minecraft",
                "deobfuscate",
                "download_dependencies"
            ],
            "function": "setup_vscode",
            "repeat_everytime": false
        },
        "download_assets": {
            "dep": [
                "download_minecraft"
            ],
            "function": "download_assets",
            "repeat_everytime": false
        },
        "run_minecraft": {
            "dep": [
                "build_all",
                "download_minecraft",
                "deobfuscate",
                "download_dependencies",
                "download_assets"
            ],
            "function": "run_minecraft",
            "repeat_everytime": false
        }
    }
}
