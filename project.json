{
    "packages": {
        "loader": {
            "source": "loader"
        },
        "agent": {
            "dep": [ "loader" ],
            "source": "agent",
            "manifest": "berryagent"
        },
        "builtins": {
            "dep": [ "loader", "deobfuscate", "download_dependencies", "download_bundled" ],
            "cps": [
                "runtime/",
                ".cache/libs/",
                ".cache/client.jar"
            ],
            "source": "builtins",
            "manifest": "builtins",
            "extras": [
                {
                    "source": "runtime/",
                    "destination": "bundled/"
                },
                {
                    "source": "manifest/builtins_services/",
                    "destination": "META-INF/services/"
                },
                {
                    "source": "manifest/builtin_mixins.json",
                    "destination": "builtin_mixins.json"
                }
            ]
        }
    },
    "runs": {
        "download_manifest": {
            "function": "download_manifest",
            "repeat_everytime": false
        },
        "build_all": {
            "dep": [
                "agent",
                "loader",
                "builtins"
            ],
            "repeat_everytime": true
        },
        "download_minecraft": {
            "dep": [ "download_manifest" ],
            "function": "download_minecraft",
            "repeat_everytime": false
        },
        "deobfuscate": {
            "dep": [ "download_minecraft" ],
            "function": "deobfuscate",
            "repeat_everytime": false
        },
        "download_dependencies": {
            "dep": [ "download_minecraft" ],
            "function": "download_dependencies",
            "repeat_everytime": false
        },
        "download_bundled": {
            "function": "download_bundled",
            "repeat_everytime": false
        },
        "setup_vscode": {
            "dep": [
                "download_minecraft",
                "deobfuscate",
                "download_dependencies",
                "download_bundled"
            ],
            "function": "setup_vscode",
            "repeat_everytime": false
        },
        "download_assets": {
            "dep": [
                "download_minecraft"
            ],
            "function": "download_assets",
            "repeat_everytime": false
        },
        "run_minecraft": {
            "dep": [
                "build_all",
                "download_minecraft",
                "deobfuscate",
                "download_dependencies",
                "download_assets"
            ],
            "function": "run_minecraft",
            "repeat_everytime": false
        }
    }
}
